@article{Jones:2015in,
author = {Jones, S and Heger, A and Georgy, C and Nishimura, N and Fryer, C and Herwig, Falk},
title = {{Code dependencies of pre-supernova evolution and nucleosynthesis in massive stars: evolution to the end of core helium burning}},
journal = {MNRAS},
year = {2015},
volume = {447},
number = {4},
pages = {3115--3129},
month = mar,
affiliation = {Department of Physics and Astronomy, University of Victoria, Victoria, BC V8W 3P6, Canada; Astrophysics Group, Lennard Jones Building, Keele University, Staffordshire ST5 5BG, UK swjones@uvic.ca},
doi = {10.1093/mnras/stu2657},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-12-23T05:55:56GMT},
date-modified = {2015-07-30T05:15:24GMT},
abstract = {Massive stars are key sources of radiative, kinetic and chemical feedback in the Universe. Grids of massive star models computed by different groups each using their own codes, input physics choices and numerical approximations, however, lead to inconsistent results for the same stars. We use three of these 1D codes - GENEC, KEPLER and MESA - to compute non-rotating stellar models of 15, 20 and 25 M⊙ and compare their nucleosynthesis. We follow the evolution from the main sequence until the end of core helium burning. The GENEC and KEPLER models hold physics assumptions used in large grids of published models. The MESA code was set up to use convective core overshooting such that the CO core masses are consistent with those obtained by GENEC. For all models, full nucleosynthesis is computed using the NuGrid post-processing tool MPPNP. We find that the surface abundances predicted by the models are in reasonable agreement. In the helium core, the standard deviation of the elemental overproduction factors for Fe to Mo is less than 30 per cent - smaller than the impact of the present nuclear physics uncertainties. For our three initial masses, the three stellar evolution codes yield consistent results. Differences in key properties of the models, e.g. helium and CO core masses and the time spent as a red supergiant, are traced back to the treatment of convection and, to a lesser extent, mass loss. The mixing processes in stars remain the key uncertainty in stellar modelling. Better constrained prescriptions are thus necessary to improve the predictive power of stellar evolution models.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2015MNRAS.447.3115J&link_type=EJOURNAL},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/91/912AFE6F-CFC3-43C9-8B6A-39852D8CB948.pdf},
file = {{912AFE6F-CFC3-43C9-8B6A-39852D8CB948.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/91/912AFE6F-CFC3-43C9-8B6A-39852D8CB948.pdf:application/pdf;912AFE6F-CFC3-43C9-8B6A-39852D8CB948.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/91/912AFE6F-CFC3-43C9-8B6A-39852D8CB948.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/mnras/stu2657}}
}

