@article{Valcarce:2014en,
author = {Valcarce, A A R and Catelan, M and Alonso-Garc{\'\i}a, J and Cort{\'e}s, C and De Medeiros, J R},
title = {{Constraints on Helium Enhancement in the Globular Cluster M4 (NGC 6121): The Horizontal Branch Test}},
journal = {ApJ},
year = {2014},
volume = {782},
number = {2},
pages = {85},
month = feb,
affiliation = {Universidade Federal do Rio Grande do Norte, Departamento de F{\'\i}sica, 59072-970 Natal, RN, Brazil ; Pontificia Universidad Cat{\'o}lica de Chile, Centro de Astroingenier{\'\i}a, Av. Vicu{\~n}a Mackena 4860, 782-0436 Macul, Santiago, Chile},
doi = {10.1088/0004-637X/782/2/85},
read = {Yes},
rating = {0},
date-added = {2014-04-04T03:13:14GMT},
date-modified = {2015-07-16T05:00:55GMT},
abstract = {Recent pieces of evidence have revealed that most, and possibly all, globular star clusters are composed of groups of stars that formed in multiple episodes with different chemical compositions. In this sense, it has also been argued that variations in the initial helium abundance (Y) from one population to the next are also the rule, rather than the exception. In the case of the metal-intermediate globular cluster M4 (NGC 6121), recent high-resolution spectroscopic observations of blue horizontal branch (HB) stars (i.e., HB stars hotter than the RR Lyrae instability strip) suggest that a large fraction of blue HB stars are second-generation stars formed with high helium abundances. In this paper, we test this scenario by using recent photometric and spectroscopic data together with theoretical evolutionary computations for different Y values. Comparing the photometric data with the theoretically derived color-magnitude diagrams, we find that the bulk of the blue HB stars in M4 have $\Delta$Y <{\textasciitilde} 0.01 with respect to the cluster's red HB stars (i.e., HB stars cooler than the RR Lyrae strip){\textemdash}a result which is corroborated by comparison with spectroscopically derived gravities and temperatures, which also favor little He enhancement. However, the possible existence of a minority population on the blue HB of the cluster with a significant He enhancement level is also discussed.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2014ApJ...782...85V&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/7F/7F70CDDF-A464-4A71-8590-983FFF4683F0.pdf},
file = {{7F70CDDF-A464-4A71-8590-983FFF4683F0.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/7F/7F70CDDF-A464-4A71-8590-983FFF4683F0.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/782/2/85}}
}

@article{Fukugita:1998bx,
