@article{Carretta:2010jk,
author = {Carretta, Eugenio and Bragaglia, A and Gratton, R G and Lucatello, S and Bellazzini, M and Catanzaro, G and Leone, F and Momany, Y. and Piotto, G. and D{\textquoteright}Orazi, V},
