@article{Tur:2009ji,
author = {Tur, Clarisse and Heger, Alexander and Austin, Sam M.},
title = {{Dependence of s-Process Nucleosynthesis in Massive Stars on Triple-Alpha and 12C($\alpha$, $\gamma$)16O Reaction Rate Uncertainties}},
journal = {ApJ},
year = {2009},
volume = {702},
number = {2},
pages = {1068--1077},
month = sep,
affiliation = {National Superconducting Cyclotron Laboratory, Michigan State University, 1 Cyclotron Laboratory, East Lansing, MI 48824-1321, USA; Joint Institute for Nuclear Astrophysics, Michigan State University, East Lansing, MI 44824, USA tur@nscl.msu.edu},
