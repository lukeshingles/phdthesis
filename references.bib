@article{Yong:2006ia,
author = {Yong, David and Aoki, Wako and Lambert, David L. and Paulson, Diane B.},
title = {{Rubidium and Lead Abundances in Giant Stars of the Globular Clusters M13 and NGC 6752}},
journal = {ApJ},
year = {2006},
volume = {639},
