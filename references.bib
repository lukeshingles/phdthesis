journal = {ApJL},
year = {2010},
volume = {719},
number = {2},
pages = {L213--L217},
month = aug,
affiliation = {INAF-Osservatorio Astronomico di Padova, vicolo dell'Osservatorio 5, I-35122, Padova, Italy valentina.dorazi@oapd.inaf.it},
