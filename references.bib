file = {{BC1A8F88-E81B-4CC8-B937-AA2559DEDE96.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/BC/BC1A8F88-E81B-4CC8-B937-AA2559DEDE96.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361:20078132}}
}

