affiliation = {Centre de Recherche Astrophysique de Lyon, UMR 5574: CNRS, Universit{\'e} de Lyon, {\'E}cole Normale Sup{\'e}rieure de Lyon, 46 all{\'e}e d'Italie, 69364 Lyon Cedex 07, France},
doi = {10.1051/0004-6361:20078096},
