journal = {A{\&}A},
year = {2001},
volume = {378},
pages = {L25--L28},
month = oct,
affiliation = {Institut d'Astronomie et d'Astrophysique, Universit{\'e} Libre de Bruxelles, CP 226, 1050 Brussels, Belgium},
doi = {10.1051/0004-6361:20011249},
read = {Yes},
rating = {0},
date-added = {2014-05-07T06:49:28GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {Contrary to previous expectations, recent evolutionary models of zero-metallicity stars show that the development of mixing episodes at the beginning of the AGB phase allows low- and intermediate-mass stars to experience thermal pulses. If these stars, like their metal-rich counterparts, also experience partial mixing of protons from the H-rich envelope into the C-rich layers at the time of the third dredge-up, an extensive neutron capture nucleosynthesis leads to the production of s-process nuclei up to Pb and Bi. Nucleosynthesis calculations based on stellar AGB models are performed assuming a parameterized H-abundance profile below the convective envelope at the time of the third dredge-up. Despite the absence of Fe-group elements, the large neutron flux resulting from the \reac{13}{C}{\alpha}n{16}{O} reaction leads to an efficient production of s-process elements starting from the neutron captures on the C-Ne isotopes. Provided partial mixing of protons takes place, it is shown that population III AGB stars should be enriched in s-process elements and overall in Pb and Bi.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2001A%26A...378L..25G&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/B0/B052AB5D-4E2C-4242-94AF-09AF5D2C2FFD.pdf},
