year = {2010},
volume = {710},
number = {1},
pages = {L67--L71},
month = feb,
affiliation = {Main Astronomical Observatory, Ukrainian National Academy of Sciences, 27 Zabolotnoho Str., Kyiv 03680, Ukraine izotov@mao.kiev.ua},
