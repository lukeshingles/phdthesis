date-modified = {2015-07-25T09:32:23GMT},
abstract = {Available data on the metal abundances of globular clusters are combined to examine the question of the existence of a metallicity gradient in the halo of the Milky Way. The average metallicity defined by the globular clusters declines slowly with galactocentric distance, following an exponential law, out to at least 100 kpc from the galactic center. At all galactocentric distances, the scatter in cluster metal abundances is large, {\textpm}0.3 in [Fe/H]. Although the gradient is weak, its existence implies that the clusters did participate in a general, homologous collapse of the Milky Way and did not form separately.},
url = {http://adsabs.harvard.edu/doi/10.1086/162136},
