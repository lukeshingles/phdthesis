@article{Boothroyd:1992gi,
author = {Boothroyd, Arnold I and Sackmann, I.-Juliana},
title = {{Breakdown of the core mass-luminosity relation at high luminosities on the asymptotic giant branch}},
journal = {ApJ},
year = {1992},
volume = {393},
pages = {L21--L24},
month = jul,
affiliation = {Canadian Institute for Theoretical Astrophysics, Toronto, Canada},
doi = {10.1086/186441},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-05-07T06:07:06GMT},
date-modified = {2015-07-21T05:14:46GMT},
abstract = {Hot bottom burning in asymptotic giant branch stars causes a breakdown of the core mass-luminosity (MC-L) relation. For stellar models with up-to-date input physics, including molecular opacities and the associated higher mixing length, the breakdown takes place at core masses greater than or approximately equal to 0.85 solar luminosity, i.e., luminosities brighter than Mbol of about -6. This occurs in stars of initial mass greater than or approximately equal to 5 solar luminosities for Population I (Z = 0.02) compositions, and greater than or approximately equal to 4 solar luminosities for Population II (Z = 0.001). For these stars, the luminosity increases much more steeply than the MC-L relation would predict, and is no longer independent of the stellar mass. Large mass loss rates can turn off hot bottom burning by significantly reducing the star's mass (and thus the temperature at the base of the convective envelope), causing the star's luminosity to decline toward the standard MC-L relation; such mass-loss rates are still an order of magnitude less than that observed in some OH/IR stars. Uncertainties in molecular opacities can significantly affect the extent of hot bottom burning, and thus the luminosities attained.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1992ApJ...393L..21B&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/62/628F4E5D-CD5B-4B7B-AB6B-DD0910BB1246.pdf},
file = {{628F4E5D-CD5B-4B7B-AB6B-DD0910BB1246.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/62/628F4E5D-CD5B-4B7B-AB6B-DD0910BB1246.pdf:application/pdf}},
