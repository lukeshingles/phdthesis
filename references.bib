file = {{0FF3B007-CD13-4704-961A-A27816DDDF00.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/0F/0FF3B007-CD13-4704-961A-A27816DDDF00.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/mnras/stu2537}}
}

