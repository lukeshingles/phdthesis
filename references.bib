@article{Goriely:2004gw,
author = {Goriely, Stephane and Siess, L},
title = {{S-process in hot AGB stars: A complex interplay between diffusive mixing and nuclear burning}},
journal = {A{\&}A},
year = {2004},
volume = {421},
pages = {L25--L28},
month = jul,
affiliation = {Institut d'Astronomie et d'Astrophysique, Universit{\'e} Libre de Bruxelles, CP 226, 1050 Brussels, Belgium},
doi = {10.1051/0004-6361:20040184},
read = {Yes},
