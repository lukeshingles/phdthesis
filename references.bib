pages = {L175--L178},
month = aug,
affiliation = {Danish AsteroSeismology Centre (DASC), Department of Physics and Astronomy, University of Aarhus, DK-8000 Aarhus C, Denmark jcd@phys.au.dk hans@phys.au.dk},
