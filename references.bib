pages = {54},
month = jul,
affiliation = {Univ Paris-Sud, Institut d'Astrophysique Spatiale, UMR 8617, CNRS, B{\^a}timent 121, 91405, Orsay Cedex, France Thierry.Appourchaux@ias.u-psud.fr; Kavli Institute for Theoretical Physics, University of California, Santa Barbara, CA, 93106-4030, USA},
doi = {10.1051/0004-6361/201218948},
read = {Yes},
