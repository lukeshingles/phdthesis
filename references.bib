file = {{C0981A35-C388-4439-9FEF-CA0A603482B9.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/C0/C0981A35-C388-4439-9FEF-CA0A603482B9.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/751/1/8}}
}

