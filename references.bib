file = {{CBCC7B12-188E-46D4-9D5C-8F97BE32C1C0.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/CB/CBCC7B12-188E-46D4-9D5C-8F97BE32C1C0.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/A6104919-EE59-43C9-B7AB-CA34D6A67465}}
