affiliation = {Universit{\"a}t Potsdam, Institut f{\"u}r Physik, Astrophysik, Am Neuen Palais 10, D-14469 Potsdam, Germany ; Astrophysikalisches Institut Potsdam (AIP), An der Sternwarte 16, D-14482 Potsdam, Germany fherwig@astro.physik.uni-potsdam.de fherwig@aip.de},
