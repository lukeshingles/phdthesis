uri = {\url{papers3://publication/doi/10.1111/j.1365-2966.2009.14394.x}}
}

@article{Brockamp:2014by,
author = {Brockamp, M and K{\"u}pper, A H W and Thies, I and Baumgardt, H and Kroupa, P},
