affiliation = {Yunnan Observatories, Chinese Academy of Sciences, Kunming 650011, China; University of Chinese Academy of Sciences, Beijing 100049, China; Key Laboratory of Structure and Evolution of Celestial Objects, Chinese Academy of Sciences, Kunming 650011, China zhuangyl@outlook.com},
