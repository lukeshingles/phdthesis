month = dec,
publisher = {IOP Publishing},
affiliation = {Division of Theoretical Astronomy, National Astronomical Observatory of Japan, Mitaka-shi, Tokyo 181-8588, Japan chiaki@th.nao.ac.jp},
doi = {10.1086/508914},
