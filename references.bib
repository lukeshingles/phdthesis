}

@article{Gingold:1976kx,
author = {Gingold, R A},
title = {{The evolutionary status of Population II Cepheids}},
journal = {ApJ},
year = {1976},
volume = {204},
pages = {116--130},
month = feb,
affiliation = {Mt. Stromlo and Siding Spring Observatoires, Canberra, Australia},
doi = {10.1086/154156},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-06-21T03:57:58GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {Several grids of post-horizontal-branch evolutionary models have been constructed assuming a range in either total mass or initial core mass on the horizontal branch. The less luminous Population II Cepheids (BL Herculis stars) are identified with stars of small envelope mass undergoing 'blueward noses' from the asymptotic giant branch due to readjustments between the hydrogen- and helium-burning shell luminosities following the exhaustion of helium in the core. The more luminous Cepheids (W Virginis stars) are identified with stars undergoing blueward loops from the second giant branch in response to helium shell flashes or finally evolving to the blue as the hydrogen-burning shell nears the stellar surface. The models predict relatively too few variables spread over too large a range in luminosity in the brighter group. Mass loss on the second giant branch is suggested to help resolve this problem. The assumption of a range in total mass on the zero-age horizontal branch produces a slightly better fit to the observed luminosity distribution of Population II Cepheids than does the assumption of a range in initial core mass.},
url = {http://adsabs.harvard.edu/doi/10.1086/154156},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/48/4895277E-B5B0-46DE-AA0C-FF1DADD4F103.pdf},
file = {{4895277E-B5B0-46DE-AA0C-FF1DADD4F103.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/48/4895277E-B5B0-46DE-AA0C-FF1DADD4F103.pdf:application/pdf;4895277E-B5B0-46DE-AA0C-FF1DADD4F103.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/48/4895277E-B5B0-46DE-AA0C-FF1DADD4F103.pdf:application/pdf}},
