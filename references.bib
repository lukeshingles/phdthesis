number = {2},
pages = {L149--L152},
month = dec,
affiliation = {INAF-Osservatorio Astronomico di Roma, Via Frascati 33, 00040 Monteporzio, Italy dantona@mporzio.astro.it ventura@mporzio.astro.it},
doi = {10.1086/499491},
read = {Yes},
