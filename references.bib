uri = {\url{papers3://publication/uuid/3047A104-1796-40F2-B0B0-8062D8FD2C83}}
}

@article{Siess:2002fq,
author = {Siess, Lionel and Livio, Mario and Lattanzio, John C.},
title = {{Structure, Evolution, and Nucleosynthesis of Primordial Stars}},
journal = {ApJ},
