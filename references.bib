@article{Chiappini:2006km,
author = {Ekstr{\"o}m, S. and Matteucci, F},
title = {{A strong case for fast stellar rotation at very low metallicities}},
journal = {A{\&}A},
year = {2006},
volume = {449},
number = {2},
pages = {L27--L30},
month = apr,
affiliation = {Osservatorio Astronomico di Trieste, via G. B. Tiepolo 11, 34131 Trieste, Italia ; Observatoire Astronomique de l'Universit{\'e} de Gen{\`e}ve, 1290 Sauverny, Switzerland Christina.Chiappini@obs.unige.ch},
doi = {10.1051/0004-6361:20064866},
read = {Yes},
