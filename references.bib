number = {2},
pages = {565--572},
month = dec,
affiliation = {Sterrekundig Instituut Utrecht, Postbus 80000, 3508 TA Utrecht, The Netherlands ; Institute of Astronomy, Madingley Road, Cambridge CB3 0HA, UK R.G.Izzard@phys.uu.nl},
