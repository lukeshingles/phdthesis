@article{1995AJ....109.1313K,
author = {Kjeldsen, Hans and Bedding, Timothy R. and Viskum, Michael and Frandsen, Soren},
title = {{Solarlike oscillations in eta Boo}},
journal = {AJ},
year = {1995},
volume = {109},
pages = {1313--1319},
month = mar,
affiliation = {Aarhus University, M{\"u}nchen, Germany},
doi = {10.1086/117363},
rating = {0},
date-added = {2012-05-30T02:21:21GMT},
date-modified = {2015-07-16T05:00:55GMT},
abstract = {We have observed evidence for p-mode oscillations in the GOIV star eta Boo (V = 2.68). This represents the first clear evidence of solarlike oscillations in a star other than the Sun. We used a new technique which measures fluctuations in the temperature of the star via their effect on the equivalent widths of the Balmer lines. The observations were obtained over six nights with the 2.5 m Nordic Optical Telescope on La Palma and consist of 12 684 low-dispersion spectra. In the power spectrum of the equivalent-width measurements, we find an excess of power at frequencies aroun 850 microHz (period 20 min) which consist of a regular series of peaks with a spacing of Delta nu = 40.3 micro Hz. We identify 13 oscillation modes, with frequency separations in agreement with theoretical expectations. Similar observations of the daytime sky show the 5 min solar oscillations at the expected frequencies.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1995AJ....109.1313K&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/51/51ED40A5-DF96-4DF5-B5A0-9EFBA1FE9FC1.pdf},
