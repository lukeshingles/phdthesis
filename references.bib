journal = {Atomic Data and Nuclear Data Tables},
year = {2000},
volume = {75},
number = {1},
pages = {1--351},
month = may,
affiliation = {Departement f{\"u}r Physik und Astronomie, Universit{\"a}t Basel, Klingelbergstrasse 82, Basel, CH-4056 Switzerland},
