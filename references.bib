@phdthesis{Kacharov:2014tj,
author = {Kacharov, N},
title = {{Chemical abundances and kinematic properties of Galactic globular clusters}},
school = {University of Heidelberg},
year = {2014},
month = dec,
publisher = {University of Heidelberg},
affiliation = {University of Heidelberg},
read = {Yes},
