@article{Denissenkov:2013dd,
author = {Denissenkov, P. A. and Herwig, Falk and Truran, J. W. and Paxton, B},
title = {{The C-flame Quenching by Convective Boundary Mixing in Super-AGB Stars and the Formation of Hybrid C/O/Ne White Dwarfs and SN Progenitors}},
journal = {ApJ},
year = {2013},
volume = {772},
number = {1},
pages = {37},
month = jul,
affiliation = {Department of Physics and Astronomy, University of Victoria, P.O. Box 3055, Victoria, BC V8W 3P6, Canada ; The Joint Institute for Nuclear Astrophysics, Notre Dame, IN 46556, USA; pavelden@uvic.ca},
doi = {10.1088/0004-637X/772/1/37},
read = {Yes},
rating = {0},
date-added = {2015-04-08T07:49:23GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {After off-center C ignition in the cores of super asymptotic giant branch (SAGB) stars, the C flame propagates all the way down to the center, trailing behind it the C-shell convective zone, and thus building a degenerate ONe core. This standard picture is obtained in stellar evolution simulations if the bottom C-shell convection boundary is assumed to be a discontinuity associated with a strict interpretation of the Schwarzschild condition for convective instability. However, this boundary is prone to additional mixing processes, such as thermohaline convection and convective boundary mixing. Using hydrodynamic simulations, we show that contrary to previous results, thermohaline mixing is too inefficient to interfere with the C-flame propagation. However, even a small amount of convective boundary mixing removes the physical conditions required for the C-flame propagation all the way to the center. This result holds even if we allow for some turbulent heat transport in the CBM region. As a result, SAGB stars build in their interiors hybrid C-O-Ne degenerate cores composed of a relatively large CO core (M CO $\approx$ 0.2 M ⊙) surrounded by a thick ONe zone ($\Delta$M ONe >{\textasciitilde} 0.85 M ⊙) with another thin CO layer above. If exposed by mass loss, these cores will become hybrid C-O-Ne white dwarfs. Otherwise, the ignition of C-rich material in the central core, surrounded by the thick ONe zone, may trigger a thermonuclear supernova (SN) explosion. The quenching of the C-flame may have implications for the ignition mechanism of SN Ia in the double-degenerate merger scenario.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...772...37D&link_type=EJOURNAL},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/77/7779336C-1905-40A3-8DF1-FF0904B382EC.pdf},
file = {{7779336C-1905-40A3-8DF1-FF0904B382EC.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/77/7779336C-1905-40A3-8DF1-FF0904B382EC.pdf:application/pdf;7779336C-1905-40A3-8DF1-FF0904B382EC.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/77/7779336C-1905-40A3-8DF1-FF0904B382EC.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/772/1/37}}
}

