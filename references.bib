file = {{47984462-ED9E-457B-9F8C-02E90D5A645D.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/47/47984462-ED9E-457B-9F8C-02E90D5A645D.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361:20030447}}
}

