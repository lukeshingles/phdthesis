@article{2007MNRAS.378.1270X,
author = {Xiong, D. R. and Deng, L.},
title = {{Non-adiabatic oscillations of red giants}},
journal = {MNRAS},
year = {2007},
volume = {378},
number = {4},
pages = {1270--1282},
month = jul,
affiliation = {Purple Mountain Observatory, Chinese Academy of Sciences, Nanjing 210008, China},
doi = {10.1111/j.1365-2966.2007.11642.x},
read = {Yes},
rating = {0},
date-added = {2012-02-27T01:25:27GMT},
date-modified = {2015-07-30T05:15:23GMT},
abstract = {Using our non-local time-dependent theory of convection, the linear non-adiabatic oscillations of 10 evolutionary model series with masses of 1-3 Msolar are calculated. The results show that there is a red giant instability strip in the lower temperature side of the Hertzsprung-Russell diagram which goes along the sequences of the red giant branch and the asymptotic giant branch. For red giants of lower luminosities, pulsation instability is found at high order overtones; the lower order modes from the fundamental to the second overtone are stable. Towards higher luminosity and lower effective temperature, instability moves to lower order modes, and the amplitude growth rate of oscillations also grows. At the high luminosity end of the strip, the fundamental and the first overtone become unstable, while all the modes above the fourth order become stable. The excitation mechanisms have been studied in detail. It is found that turbulent pressure plays a key role for excitation of red variables. The frozen convection approximation is unavailable for the low temperature stars with extended convective envelopes. In any case, this approximation can explain neither the red edge of the Cepheid instability strip, nor the blue edge of the pulsating red giant instability strip. An analytic expression of a pulsation constant as a function of stellar mass, luminosity and effective temperature is presented from this work.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2007MNRAS.378.1270X&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/54/54CD403A-DFA1-44A3-8073-DED665D0FBD0.pdf},
file = {{54CD403A-DFA1-44A3-8073-DED665D0FBD0.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/54/54CD403A-DFA1-44A3-8073-DED665D0FBD0.pdf:application/pdf}},
