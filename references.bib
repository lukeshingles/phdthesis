@article{2001ApJ...562..804K,
author = {Kwitter, Karen B and Henry, R B C},
title = {{Sulfur, Chlorine, and Argon in Planetary Nebulae. I. Observations and Abundances in a Northern Sample}},
journal = {ApJ},
year = {2001},
volume = {562},
number = {2},
pages = {804--823},
