pages = {518--530},
month = aug,
affiliation = {Institute for Advanced Study, Princeton, NJ 08540; and Institute for Cosmic Ray Research, University of Tokyo, Tanashi, Tokyo 188, Japan; fukugita@icrr.u-tokyo.ac.jp},
