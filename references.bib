@article{Pignatari:2005ci,
author = {Gallino, Roberto and Kappeler, Franz and Wiescher, M.},
title = {{Effects of uncertainties of the 22Ne($\alpha$, n)25Mg and 13C($\alpha$, n)16O reaction rates in the s-process yields}},
journal = {Nuc. Phys. A},
year = {2005},
volume = {758},
pages = {541--544},
month = jul,
affiliation = {Department of General Physics, University of Torino, Via P. Giuria 1, 10125 Torino, Italy},
doi = {10.1016/j.nuclphysa.2005.05.098},
read = {Yes},
rating = {0},
date-added = {2013-11-27T09:49:58GMT},
date-modified = {2015-07-21T05:39:05GMT},
abstract = {In the low mass star AGB phase the region between H shell and He shell (He-intershell) is affected by brief convective instabilities (Thermal Pulses), due to trigger of the He burning in the He shell. In this conditions the neutron source 22Ne($\alpha$, n)25Mg is marginally activated (releasing a short neuton burst with a peak neutron density n$\le$10 n cm), and may change isotopic ratios near s-path branchings sensitive to the peak neutron density. For each element we analyse the isotopic ratios obtained in the AGB nucleosynthesis model using three different 22Ne($\alpha$, n)25Mg rates: first,with the "standard rate", evaluated in [F. K{\"a}ppeler et al., Astrophys. J. 437, 396 (1994)]; second, with the standard rate multiplied by a factor of two, and third, the standard rate divided by a factor of two. This factor is close to the present uncertainty factor of the 22Ne($\alpha$, n)25Mg rate. The same analysis is presented for 13C($\alpha$, n)16O reaction rate, the primary neutron source in the He-intershell, which works in radiative conditions in a thin region in the top layers of the He-intershell (13C-pocket) during interpulse periods. In this case the standard rate is evaluated in [G.R. Caughlan and W.A. Fowler, Atomic Data Nucl. Data Tables 40, 283 (1988)].},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2005NuPhA.758..541P&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/54/548D51FB-706A-4775-AB81-B6A4F195209F.pdf},
