}

@article{Lugaro:2009df,
author = {Lugaro, Maria A and Campbell, S. W. and de Mink, S E},
title = {{The Mystery of CEMPs+r Stars and the Dual Core-Flash Neutron Superburst}},
journal = {Publ. Astron. Soc. Aust.},
year = {2009},
volume = {26},
number = {3},
pages = {322--326},
month = sep,
affiliation = {Centre for Stellar and Planetary Astrophysics, School of Mathematical Sciences, Monash University, Clayton, VIC 3800; Sterrekundig Instituut, University of Utrecht, Postbus 80 000, 3508 TA Utrecht, The Netherlands Maria.Lugaro@sci.monash.edu.au},
doi = {10.1071/AS08068},
rating = {0},
date-added = {2014-05-07T07:10:28GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {Carbon-enhanced metal-poor (CEMPs+r) stars show large enhancements of elements produced both by the slow and the rapid neutron capture processes (the s and r process, respectively) and represent a relatively large fraction, 30% to 50%, of the CEMP population. Many scenarios have been proposed to explain this peculiar chemical composition and most of them involve a binary companion producing the s-process elements during its Asymptotic Giant Branch (AGB) phase. The problem is that none of the proposed explanations appears to be able to account for all observational constraints, hence, alternatives are needed to be put forward and investigated. In this spirit, we propose a new scenario for the formation of CEMPs+r stars based on S. W. Campbell's finding that during the `dual core flash' in low-mass stars of extremely low metallicity, when protons are ingested in the He-flash convective zone, a `neutron superburst' is produced. Further calculations are needed to verify if this neutron superburst could make the r-process component observed in CEMPs+r, as well as their Fe abundances. The s-process component would then be produced during the following AGB phase.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009PASA...26..322L&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/1B/1B4B07CD-098D-4468-B53B-13C64FAA9667.pdf},
file = {{1B4B07CD-098D-4468-B53B-13C64FAA9667.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/1B/1B4B07CD-098D-4468-B53B-13C64FAA9667.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1071/AS08068}}
}

