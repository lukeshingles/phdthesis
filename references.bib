url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1997A%26A...324L..81H&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/C5/C5369EDF-9140-4E5F-BE22-7753495C9FDA.pdf},
