file = {{D9C8A767-BD79-4446-AC2D-11AA55A1B96A.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/D9/D9C8A767-BD79-4446-AC2D-11AA55A1B96A.pdf:application/pdf;D9C8A767-BD79-4446-AC2D-11AA55A1B96A.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/D9/D9C8A767-BD79-4446-AC2D-11AA55A1B96A.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/201425126}}
}

@article{Karakas:2012kc,
author = {Karakas, Amanda I and Garc{\'\i}a-Hern{\'a}ndez, D. A. and Lugaro, Maria A},
