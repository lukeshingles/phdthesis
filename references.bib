file = {{18E08479-15CA-4C70-AE92-14A3F8A6B5DE.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/18/18E08479-15CA-4C70-AE92-14A3F8A6B5DE.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/345728}}
}

@article{The:2007gb,
author = {The, Lih-Sin and El Eid, Mounib F and Meyer, Bradley S.},
title = {{s-Process Nucleosynthesis in Advanced Burning Phases of Massive Stars}},
