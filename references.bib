file = {{792802D0-0FF4-4D6A-B1D0-86500115C370.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/79/792802D0-0FF4-4D6A-B1D0-86500115C370.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/E86B15DB-95BA-4826-A3E8-3A4A965F7B4E}}
}

