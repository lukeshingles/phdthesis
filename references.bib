@article{2004PASA...21...97D,
author = {Davis, Tamara M and Lineweaver, Charles H},
title = {{Expanding Confusion: Common Misconceptions of Cosmological Horizons and the Superluminal Expansion of the Universe}},
journal = {Publ. Astron. Soc. Aust.},
year = {2004},
volume = {21},
number = {1},
pages = {97--109},
month = n/a,
affiliation = {University of New South Wales, Sydney NSW 2052, Australia tamarad@phys.unsw.edu.au},
