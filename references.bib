title = {{A prescription and fast code for the long-term evolution of star clusters - III. Unequal masses and stellar evolution}},
journal = {MNRAS},
year = {2014},
volume = {442},
number = {2},
pages = {1265--1285},
month = aug,
affiliation = {Institute of Astronomy, University of Cambridge, Madingley Road, Cambridge CB3 0HA, UK pera@ast.cam.ac.uk},
doi = {10.1093/mnras/stu899},
read = {Yes},
