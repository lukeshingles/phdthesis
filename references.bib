pages = {A108},
month = dec,
affiliation = {Zentrum f{\"u}r Astronomie der Universit{\"a}t Heidelberg, Landessternwarte, K{\"o}nigstuhl 12, 69117, Heidelberg, Germany a.thygesen@lsw.uni-heidelberg.de},
