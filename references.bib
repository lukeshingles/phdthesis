author = {Kiss, L. L. and Bedding, T. R.},
title = {{Red variables in the OGLE-II data base - I. Pulsations and period-luminosity relations below the tip of the red giant branch of the Large Magellanic Cloud}},
journal = {MNRAS},
year = {2003},
volume = {343},
number = {3},
