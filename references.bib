volume = {125},
number = {4},
pages = {2018--2028},
month = apr,
affiliation = {Department of Astronomy and McDonald Observatory, University of Texas at Austin, Austin, TX 78712 jensim@astro.as.utexas.edu chris@astro.as.utexas.edu},
doi = {10.1086/373926},
read = {Yes},
