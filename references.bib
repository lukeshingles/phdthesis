month = sep,
affiliation = {Research School of Astronomy and Astrophysics, Mount Stromlo Observatory, Cotter Road, Weston Creek, ACT 2611, Australia ; Universit{\"a}t W{\"u}rzburg, Emil-Fischer-Stra{\ss}e 31, D-97074~W{\"u}rzburg, Germany ; Max-Planck-Institut f{\"u}r Astrophysik, Karl-Schwarzschild-Stra{\ss}e 1, D-85748 Garching, Germany; ivo.seitenzahl@anu.edu.au},
doi = {10.1088/0004-637X/792/1/10},
read = {Yes},
rating = {0},
date-added = {2015-01-22T03:08:36GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {We revisit the evidence for the contribution of the long-lived radioactive nuclides 44Ti, 55Fe, 56Co, 57Co, and 60Co to the UVOIR light curve of SN 1987A. We show that the V-band luminosity constitutes a roughly constant fraction of the bolometric luminosity between 900 and 1900 days, and we obtain an approximate bolometric light curve out to 4334 days by scaling the late time V-band data by a constant factor where no bolometric light curve data is available. Considering the five most relevant decay chains starting at 44Ti, 55Co, 56Ni, 57Ni, and 60Co, we perform a least squares fit to the constructed composite bolometric light curve. For the nickel isotopes, we obtain best fit values of M(56Ni) = (7.1 {\textpm} 0.3) {\texttimes} 10-2 M ⊙ and M(57Ni) = (4.1 {\textpm} 1.8) {\texttimes} 10-3 M ⊙. Our best fit 44Ti mass is M(44Ti) = (0.55 {\textpm} 0.17) {\texttimes} 10-4 M ⊙, which is in disagreement with the much higher (3.1 {\textpm} 0.8) {\texttimes} 10-4 M ⊙ recently derived from INTEGRAL observations. The associated uncertainties far exceed the best fit values for 55Co and 60Co and, as a result, we only give upper limits on the production masses of M(55Co) < 7.2 {\texttimes} 10-3 M ⊙ and M(60Co) < 1.7 {\texttimes} 10-4 M ⊙. Furthermore, we find that the leptonic channels in the decay of 57Co (internal conversion and Auger electrons) are a significant contribution and constitute up to 15.5% of the total luminosity. Consideration of the kinetic energy of these electrons is essential in lowering our best fit nickel isotope production ratio to [57Ni/56Ni] = 2.5 {\textpm} 1.1, which is still somewhat high but is in agreement with gamma-ray observations and model predictions.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2014ApJ...792...10S&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/75/75AAC424-16D4-4AB6-8811-1205A0978B5B.pdf},
file = {{75AAC424-16D4-4AB6-8811-1205A0978B5B.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/75/75AAC424-16D4-4AB6-8811-1205A0978B5B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/792/1/10}}
}

