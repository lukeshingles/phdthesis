file = {{07439F50-8BF6-4752-8F74-B62D6BB44103.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/07/07439F50-8BF6-4752-8F74-B62D6BB44103.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/431740}}
}

@article{Norris:1995cy,
author = {Norris, John E. and Da Costa, Gary S},
