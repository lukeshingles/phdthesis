file = {{71D3D96D-C075-4C54-88D7-40E5C8E70C11.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/71/71D3D96D-C075-4C54-88D7-40E5C8E70C11.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/201220252}}
}

@article{Bloecker:1995ui,
author = {Bloecker, T.},
title = {{Stellar evolution of low and intermediate-mass stars. I. Mass loss on the AGB and its consequences for stellar evolution.}},
journal = {A{\&}A},
