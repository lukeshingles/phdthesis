file = {{8F42025E-37CF-4B4F-B8DD-2B640C86B72C.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/8F/8F42025E-37CF-4B4F-B8DD-2B640C86B72C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/376442}}
