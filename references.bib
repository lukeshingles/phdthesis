@article{Asplund:2009eu,
author = {Asplund, Martin and Grevesse, Nicolas and Sauval, A. Jacques and Scott, Pat},
title = {{The Chemical Composition of the Sun}},
journal = {ARA{\&}A},
year = {2009},
volume = {47},
