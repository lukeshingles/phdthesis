@article{Mitler:1972bn,
author = {Mitler, Henri E},
title = {{Cosmic-ray production of deuterium, He3, lithium, beryllium, and boron in the Galaxy}},
journal = {Ap{\&}SS},
year = {1972},
volume = {17},
number = {1},
pages = {186--218},
