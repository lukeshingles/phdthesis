}

@article{Doherty:2015ek,
author = {Doherty, Carolyn L and Gil-Pons, Pilar and Siess, Lionel and Lattanzio, John C. and Lau, Herbert H B},
title = {{Super- and massive AGB stars - IV. Final fates - initial-to-final mass relation}},
journal = {MNRAS},
year = {2015},
volume = {446},
number = {3},
pages = {2599--2612},
month = jan,
affiliation = {Monash Centre for Astrophysics (MoCA), School of Mathematical Sciences, Monash University, Victoria 3800, Australia carolyn.doherty@monash.edu},
doi = {10.1093/mnras/stu2180},
read = {Yes},
rating = {0},
date-added = {2014-10-22T10:55:34GMT},
date-modified = {2015-07-30T05:15:24GMT},
abstract = {We explore the final fates of massive intermediate-mass stars by computing detailed stellar models from the zero-age main sequence until near the end of the thermally pulsing phase. These super-asymptotic giant branch (super-AGB) and massive AGB star models are in the mass range between 5.0 and 10.0 M⊙ for metallicities spanning the range Z = 0.02-0.0001. We probe the mass limits Mup, Mn and Mmass, the minimum masses for the onset of carbon burning, the formation of a neutron star and the iron core-collapse supernovae, respectively, to constrain the white dwarf/electron-capture supernova (EC-SN) boundary. We provide a theoretical initial-to-final mass relation for the massive and ultra-massive white dwarfs and specify the mass range for the occurrence of hybrid CO(Ne) white dwarfs. We predict EC-SN rates for lower metallicities which are significantly lower than existing values from parametric studies in the literature. We conclude that the EC-SN channel (for single stars and with the critical assumption being the choice of mass-loss rate) is very narrow in initial mass, at most $\approx$0.2 M⊙. This implies that between {\~{ }}2 and 5 per cent of all gravitational collapse supernova are EC-SNe in the metallicity range Z = 0.02-0.0001. With our choice for mass-loss prescription and computed core growth rates, we find, within our metallicity range, that CO cores cannot grow sufficiently massive to undergo a Type 1.5 SN explosion.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2015MNRAS.446.2599D&link_type=EJOURNAL},
