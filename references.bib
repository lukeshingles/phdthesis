@article{Yong:2003kb,
author = {Yong, David and Lambert, David L. and Ivans, Inese I},
title = {{Magnesium Isotopic Abundance Ratios in Cool Stars}},
journal = {The Astrophysical Journal},
year = {2003},
volume = {599},
number = {2},
pages = {1357--1371},
month = dec,
affiliation = {Department of Astronomy, University of Texas at Austin, Austin, TX 78712 tofu@astro.as.utexas.edu dll@astro.as.utexas.edu},
doi = {10.1086/379369},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-07-14T06:56:38GMT},
date-modified = {2015-07-30T05:19:40GMT},
abstract = {From high-resolution spectra of 61 cool dwarfs and giants, Mg isotopic abundance ratios 24Mg:25Mg:26Mg are derived from spectral synthesis of the MgH A-X lines near 5140 {\AA}. Our sample spans the range -2.5<=[Fe/H]<=0.1, including the first measurements of Mg isotope ratios in stars with metallicities below [Fe/H]=-2.0. We confirm the decrease in 25Mg/24Mg and 26Mg/24Mg with decreasing [Fe/H], as predicted by recent models of Galactic chemical evolution in which the Mg isotopes are produced in massive stars. A subset of kinematically identified thin-disk stars have Mg isotope ratios in excellent agreement with the predictions. Within the measurement uncertainties, these thin-disk stars show no scatter about the predictions. Several of our stars are likely members of the thick disk, and their high Mg isotopic ratios may reflect the nucleosynthetic history of the thick disk, which is distinct from the predictions for, and observations of, the thin disk. For thick-disk and halo stars we find a scatter in 25Mg/24Mg and 26Mg/24Mg exceeding our measurement uncertainties and increasing with increasing metallicity. Our data suggest that an additional source of 25Mg and 26Mg is required. Intermediate-mass asymptotic giant branch stars are likely candidates.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2003ApJ...599.1357Y&link_type=EJOURNAL},
