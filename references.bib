file = {{691BFDDC-81E2-4D2D-B1DF-D643F98E54FE.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/69/691BFDDC-81E2-4D2D-B1DF-D643F98E54FE.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/513517}}
}

@article{2015arXiv150703152H,
