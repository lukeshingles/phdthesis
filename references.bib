@article{Ruiter:2009ia,
author = {Ruiter, Ashley J and Belczynski, Krzysztof and Fryer, Chris},
title = {{Rates and Delay Times of Type Ia Supernovae}},
journal = {ApJ},
year = {2009},
volume = {699},
number = {2},
pages = {2026--2036},
month = jul,
publisher = {IOP Publishing},
doi = {10.1088/0004-637X/699/2/2026},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-02-23T08:03:06GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {We analyze the evolution of binary stars to calculate synthetic rates and delay times of the most promising Type Ia Supernovae (SNe?Ia) progenitors. We present and discuss evolutionary scenarios in which a white dwarf (WD) reaches the Chandrasekhar mass and potentially explodes in a SNe?Ia. We consider Double Degenerate (DDS; merger of two WDs), Single Degenerate (SDS; WD accreting from H-rich companion), and AM Canum Venaticorum (AM CVn; WD accreting from He-rich companion) scenarios. The results are presented for two different star formation histories: burst (elliptical-like galaxies) and continuous (spiral-like galaxies). It is found that delay times for the DDS in our standard model (with common envelope efficiency ?CE = 1) follow a power-law distribution. For the SDS we note a wide range of delay times, while AM CVn progenitors produce a short burst of SNe?Ia at early times. The DDS median delay time falls between {\textasciitilde}0.5 and 1?Gyr; the SDS between {\textasciitilde}2 and 3?Gyr; and the AM CVn between {\textasciitilde}0.8 and 0.6?Gyr depending on the assumed ?CE. For a Milky-Way-like (MW-like) galaxy, we estimate the rates of SNe?Ia arising from different progenitors as: {\textasciitilde}10?4?yr?1 for the SDS and AM CVn, and {\textasciitilde}10?3?yr?1 for the DDS. We point out that only the rates for two merging carbon-oxygen WDs, the only systems found in the DDS, are consistent with the observed rates for typical MW-like spirals. We also note that DDS progenitors are the dominant population in elliptical galaxies. The fact that the delay time distribution for the DDS follows a power law implies more SNe?Ia (per unit mass) in young rather than in aged populations. Our results do not exclude other scenarios, but strongly indicate that the DDS is the dominant channel generating SNe?Ia in spiral galaxies, at least in the framework of our adopted evolutionary models. Since it is believed that WD mergers cannot produce a thermonuclear explosion given the current understanding of accreting WDs, either the evolutionary calculations along with accretion physics are incorrect, or the explosion calculations are inaccurate and need to be revisited.},
url = {http://stacks.iop.org/0004-637X/699/i=2/a=2026?key=crossref.f49e2881db95a5df5cf293f553ae02aa},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/54/547C8B1D-C682-430D-98DB-C9EA787C02DA.pdf},
file = {{547C8B1D-C682-430D-98DB-C9EA787C02DA.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/54/547C8B1D-C682-430D-98DB-C9EA787C02DA.pdf:application/pdf;547C8B1D-C682-430D-98DB-C9EA787C02DA.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/54/547C8B1D-C682-430D-98DB-C9EA787C02DA.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/699/2/2026}}
}

