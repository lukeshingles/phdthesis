@article{Carretta:2009kr,
author = {Carretta, Eugenio and Bragaglia, A and Gratton, R G and Lucatello, S and Catanzaro, G and Leone, F and Bellazzini, M and Claudi, R and D{\textquoteright}Orazi, V and Momany, Y. and Ortolani, S and Pancino, E and Piotto, G. and Recio-Blanco, A and Sabbi, E},
title = {{Na-O anticorrelation and HB. VII. The chemical composition of first and second-generation stars in 15 globular clusters from GIRAFFE spectra}},
journal = {A{\&}A},
