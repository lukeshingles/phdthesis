month = apr,
affiliation = {Astrophysics Research Centre, School of Mathematics and Physics, Queen's University Belfast, Belfast BT7 1NN, UK a.jerkstrand@qub.ac.uk},
doi = {10.1093/mnras/stv087},
read = {Yes},
