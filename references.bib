month = aug,
affiliation = {Institute for Astronomy, University of Edinburgh, Royal Observatory, Blackford Hill, Edinburgh EH9 3HJ, UK jv@roe.ac.uk},
doi = {10.1093/mnras/stu1055},
read = {Yes},
