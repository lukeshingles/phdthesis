@article{Denissenkov:2013cu,
author = {Denissenkov, Pavel A and Herwig, Falk and Bildsten, Lars and Paxton, Bill},
title = {{MESA Models of Classical Nova Outbursts: The Multicycle Evolution and Effects of Convective Boundary Mixing}},
