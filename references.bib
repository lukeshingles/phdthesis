volume = {247},
pages = {247--256},
month = jul,
annote = {A{\&}AA ID. AAA029.065.108},
affiliation = {Mount Stromlo and Siding Spring Observatoires, Canberra, Australia},
doi = {10.1086/159032},
read = {Yes},
