uri = {\url{papers3://publication/doi/10.1086/317239}}
}

@article{Tilley:2004jq,
author = {Tilley, David A and Pudritz, Ralph E},
title = {{The formation of star clusters - I. Three-dimensional simulations of hydrodynamic turbulence}},
journal = {MNRAS},
year = {2004},
volume = {353},
number = {3},
pages = {769--788},
month = sep,
affiliation = {Department of Physics and Astronomy, McMaster University, Hamilton, Ontario L8S 4M1, Canada},
doi = {10.1111/j.1365-2966.2004.08077.x},
read = {Yes},
rating = {0},
date-added = {2013-07-24T04:13:06GMT},
date-modified = {2015-07-30T05:15:23GMT},
abstract = {We present the results of a series of numerical simulations of compressible, self-gravitating hydrodynamic turbulence of cluster-forming clumps in molecular clouds. We examine the role that turbulence has in the formation of gravitationally bound cores, studying the dynamical state, internal structure and bulk properties of these cores. Complex structure in turbulent clumps is formed provided that the damping time of the turbulence, tdamp, is longer than the gravitational free-fall time tff in a region. We find a variety of density and infall velocity structures among the cores in the simulation, including cores that resemble the Larson-Penston collapse of an isothermal sphere ($\rho${\textasciitilde}r-2) and cores that resemble the McLaughlin-Pudritz collapse of logatropic spheres ($\rho${\textasciitilde}r-1). The specific angular momentum profiles range between j{\textasciitilde}r1-r2. The masses of the bound cores that form fit the initial mass function, while the specific angular momentum distribution can be fit by a broken power law. While our hydrodynamic simulations reproduce many of the observed properties of cores, we find an upper limit for the star formation efficiency in clusters of 40-50 per cent.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2004MNRAS.353..769T&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/28/2833C114-C62E-447D-B5C9-38E1845BEC96.pdf},
file = {{2833C114-C62E-447D-B5C9-38E1845BEC96.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/28/2833C114-C62E-447D-B5C9-38E1845BEC96.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1111/j.1365-2966.2004.08077.x}}
}

