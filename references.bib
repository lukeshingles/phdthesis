title = {{Effect of $^{12}C+$ $^{12}C$ Reaction {\&} Convective Mixing on the Progenitor Mass of ONe White Dwarfs}},
journal = {arXiv},
year = {2014},
eprint = {1410.1779},
eprinttype = {arxiv},
eprintclass = {1410},
pages = {1779},
