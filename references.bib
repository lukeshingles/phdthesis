title = {{Chemical gradients in the Milky Way from the RAVE data. II. Giant stars}},
journal = {A{\&}A},
year = {2014},
volume = {568},
pages = {A71},
month = aug,
affiliation = {Astronomisches Rechen-Institut, Zentrum f{\"u}r Astronomie der Universit{\"a}t Heidelberg, M{\"o}nchhofstr. 12-14, 69120, Heidelberg, Germany corrado@ari.uni-heidelberg.de},
doi = {10.1051/0004-6361/201423974},
read = {Yes},
rating = {0},
date-added = {2014-07-01T04:23:51GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {Aims: We provide new constraints on the chemo-dynamical models of the Milky Way by measuring the radial and vertical chemical gradients for the elements Mg, Al, Si, Ti, and Fe in the Galactic disc and the gradient variations as a function of the distance from the Galactic plane (Z). Methods: We selected a sample of giant stars from the RAVE database using the gravity criterium 1.7 <log g< 2.8. We created a RAVE mock sample with the Galaxia code based on the Besan{\c c}on model and selected a corresponding mock sample to compare the model with the observed data. We measured the radial gradients and the vertical gradients as a function of the distance from the Galactic plane Z to study their variation across the Galactic disc. Results: The RAVE sample exhibits a negative radial gradient of d[Fe/H]/dR = -0.054 dex kpc-1 close to the Galactic plane (| Z | < 0.4 kpc) that becomes flatter for larger | Z |. Other elements follow the same trend although with some variations from element to element. The mock sample has radial gradients in fair agreement with the observed data. The variation of the gradients with Z shows that the Fe radial gradient of the RAVE sample has little change in the range | Z | ≲ 0.6 kpc and then flattens. The iron vertical gradient of the RAVE sample is slightly negative close to the Galactic plane and steepens with | Z |. The mock sample exhibits an iron vertical gradient that is always steeper than the RAVE sample. The mock sample also shows an excess of metal-poor stars in the [Fe/H] distributions with respect to the observed data. These discrepancies can be reduced by decreasing the number of thick disc stars and increasing their average metallicity in the Besan{\c c}on model.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2014A%26A...568A..71B&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/80/80A639A5-7F54-445F-B174-67E9E9282C0C.pdf},
file = {{80A639A5-7F54-445F-B174-67E9E9282C0C.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/80/80A639A5-7F54-445F-B174-67E9E9282C0C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/201423974}}
}

@article{Nataf:2011jv,
author = {Nataf, D M and Udalski, A. and Gould, A and Pinsonneault, M H},
title = {{OGLE-III Detection of the Anomalous Galactic Bulge Red Giant Branch Bump: Evidence of Enhanced Helium Enrichment}},
journal = {ApJ},
year = {2011},
volume = {730},
number = {2},
pages = {118},
month = apr,
affiliation = {Department of Astronomy, Ohio State University, 140 West 18th Avenue, Columbus, OH 43210, USA nataf@astronomy.ohio-state.edu},
doi = {10.1088/0004-637X/730/2/118},
read = {Yes},
rating = {0},
date-added = {2014-05-05T06:31:35GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {We measure the red giant branch bump (RGBB) of the Galactic bulge, the most metal-rich RGBB ever detected. The RGBB luminosity function peaks at the expected brightness, but its number density is very low relative to Galactic globular cluster calibrations, implying the Galactic bulge has a higher helium enrichment parameter $\Delta$Y/$\Delta$Z >= 4.0 for Y {\textasciitilde} 0.35 rather than the standard 2.0 with Y = 0.27, which we suggest may be a common feature of galactic spheroids. The RGBB is (0.71 {\textpm} 0.02) mag fainter than the red clump (RC) in I toward the densest stellar regions imaged by the OGLE-III Galactic bulge photometric survey, (|l| <= 4, 2 <{\textasciitilde} |b| <= 4). The number density of RGBB stars is (12.7 {\textpm} 2.0)% that of RC stars. The brightness dispersion of the RGBB is significantly lower than that of the RC, a result that is difficult to explain as the RGBB luminosity is known to vary significantly with metallicity. Sight lines that have two RCs have two RGBBs with similar properties to one another, an expected outcome if the Milky Way's bulge is X-shaped. We also find preliminary evidence of the Galactic bulge asymptotic giant branch bump, at a brightness of {\textasciitilde}1.06 mag brighter than the RC in I and with a number density {\textasciitilde}1.5% that of the RC. Accounting for the RGBB has a small effect on the best-fit parameters of the RC, shifting its best-fit peak brightness and reducing its brightness dispersion by {\textasciitilde}0.015 mag each.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2011ApJ...730..118N&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/88/88B0FC76-6EDA-4CBA-AC1B-EF2EA4A31472.pdf},
file = {{88B0FC76-6EDA-4CBA-AC1B-EF2EA4A31472.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/88/88B0FC76-6EDA-4CBA-AC1B-EF2EA4A31472.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/730/2/118}}
}

@article{Kobayashi:1999es,
author = {Kobayashi, Chiaki and Arimoto, Nobuo},
title = {{Gradients of Absorption-Line Strengths in Elliptical Galaxies}},
journal = {ApJ},
year = {1999},
volume = {527},
number = {2},
pages = {573--599},
month = dec,
affiliation = {Department of Astronomy, School of Science, University of Tokyo 7-3-1, Hongo, Bunkyo-ku, Tokyo 113-0033, Japan; chiaki@astron.s.u-tokyo.ac.jp},
doi = {10.1086/308092},
read = {Yes},
rating = {0},
date-added = {2014-11-01T05:31:39GMT},
date-modified = {2015-07-16T05:00:55GMT},
abstract = {We have restudied line-strength gradients of 80 elliptical galaxies. Typical metallicity gradients of elliptical galaxies are $\Delta$[Fe/H]/$\Delta$logr{\textasciitilde}=-0.3, which is flatter than the gradients predicted by monolithic collapse simulations. The metallicity gradients do not correlate with any physical properties of galaxies, including central and mean metallicities, central velocity dispersions $\sigma$0, absolute B magnitudes MB, absolute effective radii Re, and dynamical masses of galaxies. By using the metallicity gradients, we have calculated mean stellar metallicities for individual ellipticals. Typical mean stellar metallicities are <[Fe/H]>{\textasciitilde}=-0.3 and range from <[Fe/H]>{\textasciitilde}=-0.8 to +0.3, which is contrary to what Gonzalez {\&} Gorgas claimed; the mean metallicities of ellipticals are not universal. The mean metallicities correlate well with $\sigma$0 and dynamical masses, though relations for MB and Re include significant scatters. We find fundamental planes defined by surface brightnesses SBe, <[Fe/H]>, and Re (or MB), the scatters of which are much smaller than those of the <[Fe/H]>-Re (or <[Fe/H]>-MB) relations. The <[Fe/H]>-log$\sigma$0 relation is nearly parallel to the [Fe/H]0-log$\sigma$0 relation but systematically lower by 0.3 dex; thus the mean metallicities are about one-half of the central values. The metallicity-mass relation or, equivalently, the color-magnitude relation of ellipticals holds not only for the central parts of galaxies but also for entire galaxies. Assuming that Mg2 and Fe1 give [Mg/H] and [Fe/H], respectively, we find <[Mg/Fe]>{\textasciitilde}=+0.2 in most of elliptical galaxies. <[Mg/Fe]> shows no correlation with galaxy mass tracers such as $\sigma$0, in contrast to what was claimed for the central [Mg/Fe]. This can be most naturally explained if the star formation had stopped in elliptical galaxies before the bulk of Type Ia supernovae began to occur. Elliptical galaxies can have significantly different metallicity gradients and <[Fe/H]>, even if they have the same galaxy mass. This may result from galaxy mergers, but no evidence is found from presently available data to support the same origin for metallicity gradients, the scatters around the metallicity-mass relation, and dynamical disturbances. This may suggest that the scatters have their origin at the formation epoch of galaxies.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1999ApJ...527..573K&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/CC/CC6CEF3A-B346-46EA-875D-BDB4F084F078.pdf},
file = {{CC6CEF3A-B346-46EA-875D-BDB4F084F078.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/CC/CC6CEF3A-B346-46EA-875D-BDB4F084F078.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/308092}}
}

@article{Battersby:2014gp,
author = {Battersby, Cara and Ginsburg, Adam and Bally, John and Longmore, Steve and Dunham, Miranda and Darling, Jeremy},
title = {{The Onset of Massive Star Formation: The Evolution of Temperature and Density Structure in an Infrared Dark Cloud}},
journal = {ApJ},
year = {2014},
volume = {787},
number = {2},
pages = {113},
month = jun,
affiliation = {Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138, USA ; Center for Astrophysics and Space Astronomy, University of Colorado, UCB 389, Boulder, CO 80309, USA},
doi = {10.1088/0004-637X/787/2/113},
read = {Yes},
rating = {0},
date-added = {2014-05-16T03:49:53GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {We present new NH3 (1, 1), (2, 2), and (4, 4) observations from the Karl G. Jansky Very Large Array compiled with work in the literature to explore the range of conditions observed in young, massive star-forming regions. To sample the effects of evolution independent from those of distance/resolution, abundance, and large-scale environment, we compare clumps in different evolutionary stages within a single infrared dark cloud (IRDC), G32.02+0.06. We find that the early stages of clustered star formation are characterized by dense, parsec-scale filamentary structures interspersed with complexes of dense cores (<0.1 pc cores clustered in complexes separated by {\textasciitilde}1 pc) with masses from about 10 to 100 M ⊙. The most quiescent core is the most extended while the star forming cores are denser and more compact, showing very similar column density structure before and shortly after the onset of massive star formation, with peak surface densities $\Sigma$ >{\textasciitilde} 1 g cm-2. Quiescent cores and filaments show smoothly varying temperatures from 10 to 20 K, rising to over 40 K in star-forming cores. We calculate virial parameters for 16 cores and find that the level of support provided by turbulence is generally insufficient to support them against gravitational collapse (lang$\alpha$virrang {\textasciitilde} 0.6). The star-forming filaments show smooth velocity fields, punctuated by discontinuities at the sites of active star formation. We discuss the massive molecular filament (M {\textasciitilde} 105 M ⊙, length >60 pc) hosting the IRDC, hypothesizing that it may have been shaped by previous generations of massive stars.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2014ApJ...787..113B&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/39/39282D19-7D1F-4711-AA27-D26AB6123F1B.pdf},
file = {{39282D19-7D1F-4711-AA27-D26AB6123F1B.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/39/39282D19-7D1F-4711-AA27-D26AB6123F1B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/787/2/113}}
}

@article{2015ApJ...802...16Y,
author = {Yoon, Sung-Chul and Kang, Jisu and Kozyreva, Alexandra},
title = {{Can Very Massive Population III Stars Produce a Super-Collapsar?}},
journal = {ApJ},
year = {2015},
volume = {802},
number = {1},
pages = {16},
month = mar,
affiliation = {Department of Physics and Astronomy, Seoul National University, Gwanak-gu, Gwanak-ro1, 151-742, Seoul, Korea yoon@astro.snu.ac.kr},
doi = {10.1088/0004-637X/802/1/16},
read = {Yes},
rating = {0},
date-added = {2015-04-07T12:04:47GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {A fraction of the first generation of stars in the early universe may be very massive (≳ 300 {{M}⊙ }) as they form in metal-free environments. Formation of black holes from these stars can be accompanied by supermassive collapsars to produce long gamma-ray bursts of a unique type having a very high total energy ({\~{ }} {{10}54} erg) as recently suggested by several authors. We present new stellar evolution models of very massive Population III stars including the effect of rotation to provide theoretical constraints on super-collapsar progenitors. We find that the angular momentum condition for a super-collapsar can be fulfilled if magnetic torques are ignored, in which case Eddington-Sweet circulations play the dominant role for the transport of angular momentum. We further find that the initial mass range for super-collapsar progenitors would be limited to 300 {{M}⊙ }≲ M≲ 700 {{M}⊙ }. However, all of our very massive star models of this mass range end their lives as red supergiants rather than blue supergiants, in good agreement with most of the previous studies. The predicted final fate of these stars is either a jet-powered type IIP supernova or an ultra-long, relatively faint gamma-ray transient, depending on the initial amount of angular momentum.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2015ApJ...802...16Y&link_type=EJOURNAL},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/74/74A6EBE8-D80C-47AD-B4C3-721C7AAC1111.pdf},
file = {{74A6EBE8-D80C-47AD-B4C3-721C7AAC1111.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/74/74A6EBE8-D80C-47AD-B4C3-721C7AAC1111.pdf:application/pdf;74A6EBE8-D80C-47AD-B4C3-721C7AAC1111.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/74/74A6EBE8-D80C-47AD-B4C3-721C7AAC1111.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/802/1/16}}
}

@article{Melendez:2008dn,
author = {Mel{\'e}ndez, J and Asplund, Martin and Alves-Brito, Alan and Cunha, K and Barbuy, B and Bessell, M. S. and Freeman, K. C. and Ram{\'\i}rez, I and Smith, Verne V. and Yong, David},
title = {{Chemical similarities between Galactic bulge and local thick disk red giant stars}},
journal = {A{\&}A},
year = {2008},
volume = {484},
number = {3},
pages = {L21--L25},
month = jun,
affiliation = {Centro de Astrof{\'\i}sica da Universidade do Porto, Rua das Estrelas, 4150-762 Porto, Portugal; Research School of Astronomy and Astrophysics, The Australian National University, Cotter Road, Weston, ACT 2611, Australia},
doi = {10.1051/0004-6361:200809398},
rating = {0},
date-added = {2014-11-23T07:46:48GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {Context: The evolution of the Milky Way bulge and its relationship with the other Galactic populations is still poorly understood. The bulge has been suggested to be either a merger-driven classical bulge or the product of a dynamical instability of the inner disk. Aims: To probe the star formation history, the initial mass function and stellar nucleosynthesis of the bulge, we performed an elemental abundance analysis of bulge red giant stars. We also completed an identical study of local thin disk, thick disk and halo giants to establish the chemical differences and similarities between the various populations. Methods: High-resolution infrared spectra of 19 bulge giants and 49 comparison giants in the solar neighborhood were acquired with Gemini/Phoenix. All stars have similar stellar parameters but cover a broad range in metallicity. A standard 1D local thermodynamic equilibrium analysis yielded the abundances of C, N, O and Fe. A homogeneous and differential analysis of the bulge, halo, thin disk and thick disk stars ensured that systematic errors were minimized. Results: We confirm the well-established differences for [O/Fe] (at a given metallicity) between the local thin and thick disks. For the elements investigated, we find no chemical distinction between the bulge and the local thick disk, which is in contrast to previous studies relying on literature values for disk dwarf stars in the solar neighborhood. Conclusions: Our findings suggest that the bulge and local thick disk experienced similar, but not necessarily shared, chemical evolution histories. We argue that their formation timescales, star formation rates and initial mass functions were similar.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2008A%26A...484L..21M&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/01/01CE41BA-BF8F-4B0D-B8EB-0008252779D9.pdf},
file = {{01CE41BA-BF8F-4B0D-B8EB-0008252779D9.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/01/01CE41BA-BF8F-4B0D-B8EB-0008252779D9.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361:200809398}}
}

