@article{2002MNRAS.334..498Z,
author = {Zijlstra, Albert A. and Bedding, T. R. and Mattei, J. A.},
title = {{The evolution of the Mira variable R Hydrae}},
journal = {MNRAS},
year = {2002},
volume = {334},
