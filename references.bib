file = {{6C255528-93D6-450E-8BBE-DF8876CD434B.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/6C/6C255528-93D6-450E-8BBE-DF8876CD434B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/303794}}
}

