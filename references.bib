@article{Dupree:2013cb,
author = {Dupree, A K and Avrett, E H},
title = {{Direct Evaluation of the Helium Abundances in Omega Centauri}},
journal = {ApJL},
year = {2013},
volume = {773},
number = {2},
pages = {L28},
month = aug,
affiliation = {Harvard-Smithsonian Center for Astrophysics, Cambridge, MA 02138, USA},
doi = {10.1088/2041-8205/773/2/L28},
read = {Yes},
rating = {0},
date-added = {2014-09-15T03:52:45GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {A direct measure of the helium abundances from the near-infrared transition of He I at 1.08 $\mu$m is obtained for two nearly identical red giant stars in the globular cluster Omega Centauri. One star exhibits the He I line; the line is weak or absent in the other star. Detailed non-local thermal equilibrium semi-empirical models including expansion in spherical geometry are developed to match the chromospheric H$\alpha$, H$\beta$, and Ca II K lines, in order to predict the helium profile and derive a helium abundance. The red giant spectra suggest a helium abundance of Y <= 0.22 (LEID 54064) and Y = 0.39-0.44 (LEID 54084) corresponding to a difference in the abundance $\Delta$Y >= 0.17. Helium is enhanced in the giant star (LEID 54084) that also contains enhanced aluminum and magnesium. This direct evaluation of the helium abundances gives observational support to the theoretical conjecture that multiple populations harbor enhanced helium in addition to light elements that are products of high-temperature hydrogen burning. We demonstrate that the 1.08 $\mu$m He I line can yield a helium abundance in cool stars when constraints on the semi-empirical chromospheric model are provided by other spectroscopic features.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...773L..28D&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/F2/F2953B89-D4C8-4B29-B10E-6F845D9EFABA.pdf},
file = {{F2953B89-D4C8-4B29-B10E-6F845D9EFABA.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/F2/F2953B89-D4C8-4B29-B10E-6F845D9EFABA.pdf:application/pdf}},
