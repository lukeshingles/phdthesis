uri = {\url{papers3://publication/doi/10.1086/174476}}
}

@article{Sills:2010eg,
author = {Sills, Alison and Glebbeek, Evert},
title = {{Multiple populations in globular clusters: the possible contributions of stellar collisions}},
journal = {MNRAS},
year = {2010},
volume = {407},
number = {1},
pages = {277--284},
month = sep,
affiliation = {Department of Physics and Astronomy, McMaster University, Hamilton, Ontario L8S 4M1, Canada},
doi = {10.1111/j.1365-2966.2010.16876.x},
read = {Yes},
rating = {0},
date-added = {2013-08-04T08:18:16GMT},
date-modified = {2015-07-30T05:15:23GMT},
abstract = {Globular clusters were thought to be simple stellar populations, but recent photometric and spectroscopic evidence suggests that the clusters' early formation history was more complicated. In particular, clusters show star-to-star abundance variations, and multiple sequences in their colour-magnitude diagrams. These effects seem to be restricted to globular clusters, and are not found in open clusters or the field. In this paper, we combine the two competing models for these multiple populations and include a consideration of the effects of stellar collisions. Collisions are one of the few phenomena which occur solely in dense stellar environments like (proto)globular clusters. We find that runaway collisions between massive stars can produce material which has abundances comparable to the observed second generations, but that very little total mass is produced by this channel. We then add the contributions of rapidly rotating massive stars (under the assumption that massive stars are spun up by collisions and interactions), and the contribution of asymptotic giant branch stars. We find that collisions can help produce the extreme abundances which are seen in some clusters. However, the total amount of material produced in these generations is still too small (by at least a factor of 10) to match the observations. We conclude with a discussion of the additional effects which probably need to be considered to solve this particular problem.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010MNRAS.407..277S&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/7D/7D6BF54B-D53A-4946-9E83-4CF052588D28.pdf},
file = {{7D6BF54B-D53A-4946-9E83-4CF052588D28.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/7D/7D6BF54B-D53A-4946-9E83-4CF052588D28.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1111/j.1365-2966.2010.16876.x}}
}

@article{The:2000cf,
author = {The, Lih-Sin and El Eid, M F and Meyer, B S},
title = {{A New Study of s-Process Nucleosynthesis in Massive Stars}},
