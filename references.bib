pages = {68},
month = mar,
affiliation = {Dipartimento di Fisica {\&} Astronomia, Alma Mater Studiorum, Universit{\`a} di Bologna, Viale Berti Pichat 6/2, I-40127 Bologna, Italy},
doi = {10.1088/0004-637X/801/1/68},
read = {Yes},
rating = {0},
date-added = {2015-01-15T22:25:41GMT},
date-modified = {2015-07-16T05:00:55GMT},
abstract = {We derive [K/Fe] abundance ratios for 119 stars in the globular cluster NGC 2808, all of them having O, Na, Mg, and Al abundances homogeneously measured in previous works. We detect an intrinsic star-to-star spread in the potassium abundance. Moreover [K/Fe] abundance ratios display statistically significant correlations with [Na/Fe] and [Al/Fe], and anti-correlations with [O/Fe] and [Mg/Fe]. All the four Mg deficient stars ([Mg/Fe] < 0.0) discovered so far in NGC 2808 are enriched in K by {\textasciitilde}0.3 dex with respect to those with normal [Mg/Fe]. NGC 2808 is the second globular cluster, after NGC 2419, where a clear Mg-K anti-correlation is detected, albeit of weaker amplitude. The simultaneous correlation/anti-correlation of [K/Fe] with all the light elements usually involved in the chemical anomalies observed in globular cluster stars strongly support the idea that these abundance patterns are due to the same self-enrichment mechanism that produces Na-O and Mg-Al anti-correlations. This finding suggests that detectable spreads in K abundances may be typical in the massive globular clusters where the self-enrichment processes are observed to produce their most extreme manifestations. Based on data obtained at the ESO Very Large Telescope under the programs 072.D-0507 and 091.D-0329.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2015ApJ...801...68M&link_type=EJOURNAL},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/29/294AE214-D2CA-4E49-8FCE-44847C9EC702.pdf},
file = {{294AE214-D2CA-4E49-8FCE-44847C9EC702.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/29/294AE214-D2CA-4E49-8FCE-44847C9EC702.pdf:application/pdf;294AE214-D2CA-4E49-8FCE-44847C9EC702.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/29/294AE214-D2CA-4E49-8FCE-44847C9EC702.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/801/1/68}}
}

