journal = {MNRAS},
year = {2013},
volume = {436},
number = {2},
pages = {1286--1301},
month = dec,
affiliation = {Research School of Astronomy and Astrophysics, Australian National University, Canberra, ACT 2611, Australia; ARC Centre of Excellence for All-sky Astrophysics (CAASTRO), Australian National University, Canberra, ACT 2611, Australia; ben@mso.anu.edu.au},
