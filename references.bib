@article{Schroder:2005im,
author = {Schr{\"o}der, K P and Cuntz, M},
title = {{A New Version of Reimers' Law of Mass Loss Based on a Physical Approach}},
journal = {ApJ},
year = {2005},
volume = {630},
number = {1},
pages = {L73--L76},
month = sep,
affiliation = {Astronomy Centre, University of Sussex, Falmer, Brighton BN1 9QH, England, UK kps@central.susx.ac.uk},
doi = {10.1086/491579},
rating = {0},
date-added = {2014-05-10T08:35:59GMT},
date-modified = {2015-07-16T05:00:55GMT},
abstract = {We present a new semiempirical relation for the mass loss of cool stellar winds, which so far has frequently been described by ``Reimers' law.'' Originally, this relation was based solely on dimensional scaling arguments without any physical interpretation. In our approach, the wind is assumed to result from the spillover of the extended chromosphere, possibly associated with the action of waves, especially Alfv{\'e}n waves, which are used as guidance in the derivation of the new formula. We obtain a relation akin to the original Reimers law, but which includes two new factors. They reflect how the chromospheric height depends on gravity and how the mechanical energy flux depends, mainly, on the effective temperature. The new relation is tested and sensitively calibrated by modeling the blue end of the horizontal branch of globular clusters. The most significant difference from mass-loss rates predicted by the Reimers relation is an increase by up to a factor of 3 for luminous late-type (super)giants, in good agreement with observations.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2005ApJ...630L..73S&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/83/838E8617-CC06-4AAB-AD1B-C9D797A33887.pdf},
file = {{838E8617-CC06-4AAB-AD1B-C9D797A33887.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/83/838E8617-CC06-4AAB-AD1B-C9D797A33887.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/491579}}
}

@article{Ventura:2013ct,
author = {Ventura, P and Di Criscienzo, M and Carini, R and D'Antona, Francesca},
title = {{Yields of AGB and SAGB models with chemistry of low- and high-metallicity globular clusters}},
journal = {MNRAS},
year = {2013},
volume = {431},
number = {4},
pages = {3642--3653},
month = jun,
affiliation = {INAF - Osservatorio Astronomico di Roma, Via Frascati 33, I-00040 Monte Porzio Catone, Italy; paolo.ventura@oa-roma.inaf.it},
doi = {10.1093/mnras/stt444},
read = {Yes},
rating = {0},
date-added = {2013-09-14T04:05:54GMT},
date-modified = {2015-07-30T05:15:23GMT},
abstract = {We present yields from stars of mass in the range M⊙ $\le$ M $\le$ 8 M⊙ of metallicities Z = 3 {\texttimes} 10-4 and 8 {\texttimes} 10-3, thus encompassing the chemistry of low- and high-Z globular clusters. The yields are based on full evolutionary computations, following the evolution of the stars from the pre-main sequence through the asymptotic giant branch phase, until the external envelope is lost. Independent of metallicity, stars with M < 3 M⊙ are dominated by third dredge-up, thus ejecting into their surroundings gas enriched in carbon and nitrogen. Conversely, hot bottom burning is mainly responsible for the modification of the surface chemistry of more massive stars, whose mass exceeds 3 M⊙: their gas shows traces of proton-capture nucleosynthesis. The extent of hot bottom burning turns out to be strongly dependent on metallicity. Models with Z = 8 {\texttimes} 10-3 achieve a modest depletion of oxygen, barely reaching -0.3 dex, and do not activate the Mg-Al chain. Low-Z models with Z = 3 {\texttimes} 10-4 achieve a strong nucleosynthesis at the bottom of the envelope, with a strong destruction of the surface oxygen and magnesium; the most extreme chemistry is reached for models of mass {\~{ }}6 M⊙, where $\delta$[O/Fe] {\~{ }} -1.2 and $\delta$[Mg/Fe] {\~{ }} -0.6. Sodium is found to be produced in modest quantities at these low Zs, because the initial increase due to the combined effect of the second dredge-up and of 22Ne burning is compensated by the later destruction via proton capture. A great increase by a factor of {\~{ }}10 in the aluminium content of the envelope is also expected. These results can be used to understand the role played by intermediate-mass stars in the self-enrichment scenario of globular clusters: the results from spectroscopic investigations of stars belonging to the second generation of clusters with different metallicity will be used as an indirect test of the reliability of the present yields. The treatment of mass loss and convection is confirmed as the main uncertainties affecting the results obtained in the context of the modelling of the thermal pulses phase. An indirect proof of this comes from the comparison with other investigations in the literature, based on a different prescription for the efficiency of convection in transporting energy and using a different recipe to determine the mass-loss rate.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013MNRAS.431.3642V&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/8F/8FCC37BB-5CAF-4549-A87C-0C0C78EC4E72.pdf},
file = {{8FCC37BB-5CAF-4549-A87C-0C0C78EC4E72.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/8F/8FCC37BB-5CAF-4549-A87C-0C0C78EC4E72.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/mnras/stt444}}
}

@article{Vesperini:2013fd,
author = {Vesperini, Enrico and McMillan, Stephen L. W. and D'Antona, Francesca and D'Ercole, Annibale},
title = {{Dynamical evolution and spatial mixing of multiple population globular clusters}},
journal = {MNRAS},
year = {2013},
volume = {429},
number = {3},
pages = {1913--1921},
month = mar,
affiliation = {Department of Astronomy, Indiana University, Bloomington, IN 47405, USA; evesperini@astro.indiana.edu},
doi = {10.1093/mnras/sts434},
read = {Yes},
rating = {0},
date-added = {2014-07-06T23:46:50GMT},
date-modified = {2015-07-30T05:15:24GMT},
abstract = {Numerous spectroscopic and photometric observational studies have provided strong evidence for the widespread presence of multiple stellar populations in globular clusters. In this paper, we study the long-term dynamical evolution of multiple population clusters, focusing on the evolution of the spatial distributions of the first- (FG) and second-generation (SG) stars. In previous studies, we have suggested that SG stars formed from the ejecta of FG AGB stars are expected initially to be concentrated in the cluster inner regions. Here, by means of N-body simulations, we explore the time-scales and the dynamics of the spatial mixing of the FG and the SG populations and their dependence on the SG initial concentration. Our simulations show that, as the evolution proceeds, the radial profile of the SG/FG number ratio, NSG/NFG, is characterized by three regions: (1) a flat inner part; (2) a declining part in which FG stars are increasingly dominant and (3) an outer region where the NSG/NFG profile flattens again (the NSG/NFG profile may rise slightly again in the outermost cluster regions). Until mixing is complete and the NSG/NFG profile is flat over the entire cluster, the radial variation of NSG/NFG implies that the fraction of SG stars determined by observations covering a limited range of radial distances is not, in general, equal to the SG global fraction, (NSG/NFG)glob. The distance at which NSG/NFG equals (NSG/NFG)glob is approximately between 1 and 2 cluster half-mass radii. The time-scale for complete mixing depends on the SG initial concentration, but in all cases complete mixing is expected only for clusters in advanced evolutionary phases, having lost at least 60-70 per cent of their mass due to two-body relaxation (in addition to the early FG loss due to the cluster expansion triggered by SNII ejecta and gas expulsion).The results of our simulations suggest that in many Galactic globular clusters the SG should still be more spatially concentrated than the FG.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013MNRAS.429.1913V&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/05/05BFEE5C-9986-48C9-AE4F-FC132D35229B.pdf},
file = {{05BFEE5C-9986-48C9-AE4F-FC132D35229B.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/05/05BFEE5C-9986-48C9-AE4F-FC132D35229B.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/mnras/sts434}}
}

@article{2010JCAP...02..008S,
author = {Stern, Daniel and Jimenez, Raul and Verde, Licia and Kamionkowski, Marc and Stanford, S. Adam},
title = {{Cosmic chronometers: constraining the equation of state of dark energy. I: H(z) measurements}},
journal = {Journal of Cosmology and Astroparticle Physics},
year = {2010},
volume = {02},
number = {0},
pages = {008},
month = feb,
affiliation = {Jet Propulsion Laboratory, California Institute of Technology, Mail Stop 169-506, Pasadena CA-91109, USA},
doi = {10.1088/1475-7516/2010/02/008},
read = {Yes},
rating = {0},
date-added = {2012-05-22T06:25:33GMT},
date-modified = {2015-07-25T09:32:19GMT},
abstract = {We present new determinations of the cosmic expansion history from red-envelope galaxies. We have obtained for this purpose high-quality spectra with the Keck-LRIS spectrograph of red-envelope galaxies in 24 galaxy clusters in the redshift range 0.2 < z < 1.0. We complement these Keck spectra with high-quality, publicly available archival spectra from the SPICES and VVDS surveys. We improve over our previous expansion history measurements in Simon et al. (2005) by providing two new determinations of the expansion history: H(z) = 97{\textpm}62 km sec-1 Mpc-1 at z simeq 0.5 and H(z) = 90{\textpm}40 km sec-1 Mpc-1 at z simeq 0.9. We discuss the uncertainty in the expansion history determination that arises from uncertainties in the synthetic stellar-population models. We then use these new measurements in concert with cosmic-microwave-background (CMB) measurements to constrain cosmological parameters, with a special emphasis on dark-energy parameters and constraints to the curvature. In particular, we demonstrate the usefulness of direct H(z) measurements by constraining the dark-energy equation of state parameterized by w0 and wa and allowing for arbitrary curvature. Further, we also constrain, using only CMB and H(z) data, the number of relativistic degrees of freedom to be 4{\textpm}0.5 and their total mass to be < 0.2 eV, both at 1$\sigma$.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010JCAP...02..008S&link_type=ABSTRACT},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/12/1222CDE3-3179-4846-BC7A-AB2FDF6E529A.pdf},
file = {{1222CDE3-3179-4846-BC7A-AB2FDF6E529A.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/12/1222CDE3-3179-4846-BC7A-AB2FDF6E529A.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/1475-7516/2010/02/008}}
}

@article{Bertout:2005hs,
