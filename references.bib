@article{1996Natur.381..210G,
author = {Gloeckler, George and Geiss, Johannes},
title = {{Abundance of 3He in the local interstellar cloud}},
journal = {Nature},
year = {1996},
volume = {381},
number = {6},
pages = {210--212},
month = may,
doi = {10.1038/381210a0},
read = {Yes},
