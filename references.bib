pages = {A1},
month = may,
annote = {19 pages, 9 figures, accepted for A{\&}amp;A

},
affiliation = {Argelander Institute for Astronomy, University of Bonn, Auf dem Huegel 71, 53121, Bonn, Germany h.lau@monash.edu; Monash Centre for Astrophysics, School of Mathematical Sciences, Monash University, Victoria, 3800, Australia},
doi = {10.1051/0004-6361/201218826},
language = {English},
read = {Yes},
