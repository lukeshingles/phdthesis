}

@article{Roederer:2012ic,
author = {Roederer, Ian U and Lawler, James E and Sobeck, Jennifer S and Beers, Timothy C and Cowan, John J and Frebel, Anna and Ivans, Inese I and Schatz, Hendrik and Sneden, Christopher and Thompson, Ian B},
title = {{New Hubble Space Telescope Observations of Heavy Elements in Four Metal-Poor Stars}},
journal = {ApJS},
year = {2012},
