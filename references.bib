title = {{Spectroscopy of the Type Ia supernova 2011fe past 1000 d}},
journal = {MNRASL},
year = {2015},
volume = {448},
number = {1},
pages = {L48--L52},
month = mar,
affiliation = {European Southern Observatory, Karl-Schwarzschild-Str. 2, D-85748 Garching, Germany; Max-Planck-Institut f{\"u}r Astrophysik, Karl-Schwarzschild-Str. 1, D-85741 Garching, Germany tauben@mpa-garching.mpg.de},
doi = {10.1093/mnrasl/slu201},
language = {English},
read = {Yes},
