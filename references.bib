@article{McWilliam:2005jp,
author = {McWilliam, Andrew and Smecker-Hane, Tammy A},
title = {{The Unusual Abundance of Copper in the Sagittarius Dwarf Spheroidal Galaxy and Implications for the Origin of $\omega$ Centauri}},
journal = {ApJ},
year = {2005},
volume = {622},
number = {1},
pages = {L29--L32},
month = mar,
publisher = {IOP Publishing},
doi = {10.1086/429407},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-04-15T03:49:01GMT},
date-modified = {2015-07-16T05:00:55GMT},
abstract = {We present copper abundances for 14 red giant stars in the Sagittarius dwarf spheroidal galaxy (Sgr dSph), showing that [Cu/Fe] is deficient by {\textasciitilde}0.5 dex, relative to the Galactic trend. This is most easily understood as due to an enhanced contribution of iron peak nucleosynthesis products from Type Ia supernovae (SNe Ia). The deficient [Cu/Fe] ratios might also be the result of a metallicity-dependent yield from SNe Ia, similar to previous conclusions for Mn, although SN Ia nucleosynthesis predictions suggest a negligible Cu yield. The enhanced SN Ia products, suggested by our low [Cu/Fe] ratios, fit a leaky box chemical evolution scenario for the Sgr dSph, where ejecta from the old, metal-poor, population overwhelmed nucleosynthesis products from younger generations, resulting in young stars with uncharacteristic compositions. The only other system known to have unusually low [Cu/Fe] is the Galactic globular cluster $\omega$ Cen, which, like the Sagittarius dwarf galaxy, has strong enhancements of s-process elements. Thus, our copper abundances lend support to the idea that $\omega$ Cen is the remaining nucleus of an accreted dwarf galaxy.},
url = {http://stacks.iop.org/1538-4357/622/i=1/a=L29},
local-url = {file://localhost/Users/lukes/Dropbox/Apps/Library.papers3/Files/68/6882034E-C292-4D85-B922-615C09FD06A4.pdf},
file = {{6882034E-C292-4D85-B922-615C09FD06A4.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/68/6882034E-C292-4D85-B922-615C09FD06A4.pdf:application/pdf;6882034E-C292-4D85-B922-615C09FD06A4.pdf:/Users/lukes/Dropbox/Apps/Library.papers3/Files/68/6882034E-C292-4D85-B922-615C09FD06A4.pdf:application/pdf}},
